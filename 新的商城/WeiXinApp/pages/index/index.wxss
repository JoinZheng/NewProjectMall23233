.bannerList {
  margin: 0;
  width: 100%;
  height: 467rpx;
}

.bannerList swiper {
  height: 467rpx;
}

.bannerList .targetImg {
  width: 100%;
  height: 467rpx;
}

.hotAction image {
  width: 100%;
  height: 160rpx;
}

.hotList {
  margin: 0rpx 10rpx;
  white-space: nowrap;
}

.hotList .hotItem {
  width: 280rpx;
  display: inline-block;
  flex: auto;
  margin: 0rpx 20rpx 20rpx 20rpx;
}

.hotItem {
  position: relative;
}

.hotList .hotItem .imgBack {
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 280rpx 280rpx;
  -moz-background-size: 100% 100%;
  height: 280rpx;
  width: 280rpx;
  position: absolute;
  z-index: 100;
  top: 0rpx;
}

.hotList .hotItem image {
  height: 280rpx;
  width: 280rpx;
}

.hotList .hotItem .hotTitle {
  font-size: 30rpx;
  white-space: pre-wrap;
  word-break: break-all;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  width: 280rpx;
  height: 84rpx;
}

.hotList .hotItem .hotText {
  font-size: 24rpx;
  border: 1rpx solid #CDCDCD;
  text-align: center;
}

.hotList .hotItem .hotText .white {
  padding: 4rpx;
}

.hotList .hotItem .hotText .red {
  background-color: #3981F4;
  padding: 4rpx;
  color: #ffffff;
}

.hotmore {
  font-size: 30rpx;
  text-align: center;
  padding: 20rpx;
}

.dataTab {
  font-size: 24rpx;
  text-align: center;
  padding: 10rpx;
  margin: 0rpx 10rpx;
}

.dataTab .tabSelected {
  background-color: #3981F4;
  border: 1px solid #3981F4;
  color: #ffffff;
  padding: 10rpx;
}

.dataTab .tab {
  border: 1px solid #CDCDCD;
  padding: 10rpx;
}

.dataList {
  border-top: 1px solid #CDCDCD;
  border-left: 1px solid #CDCDCD;
  border-right: 1px solid #CDCDCD;
  margin: 0rpx 20rpx;
  font-size: 24rpx;
}

.dataList .dataItem {
  padding: 40rpx 10rpx;
  border-bottom: 1px solid #CDCDCD;
}

.dataList image {
  width: 100%;
  height: 140rpx;
}

.dataList .text {
  margin-bottom: 30rpx;
  min-height: 56rpx;
  margin-left: 20rpx;
}

.dataList .dataCircle {
  color: #3981F4;
  margin-left: 20rpx;
}

::-webkit-scrollbar {
  display: none;
}

.van-enter-active-class,
.van-leave-active-class {
  transition-property: background-color, transform;
}

.van-enter-class,
.van-leave-to-class {
  background-color: red;
  transform: rotate(-360deg) translate3d(-100%, -100%, 0);
}