
page {
	height: auto;
	min-height: 100%;
}
.uni-card {
	box-shadow: none;
}
.uni-list:after {
	height: 0;
}
.uni-list:before {
	height: 0;
}
.uni-media-list-logo {
	width: 180rpx;
	height: 140rpx;
}
.uni-media-list-body {
	height: auto;
	justify-content: space-around;
}
.uni-media-list-text-top {
	height: 74rpx;
	font-size: 28rpx;
	overflow: hidden;
}
.uni-media-list-text-bottom {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}

